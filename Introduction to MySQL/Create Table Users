CREATE TABLE users(
id INT PRIMARY KEY AUTO_INCREMENT NOT NULL,
username VARCHAR(30) UNIQUE,
`password` VARCHAR(26),
profile_picture BLOB(900),
last_login_time DATETIME,
`is_deleted` BIT(1)
);

